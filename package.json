{
  "name": "antd-admin",
  "version": "5.0.0-beta",
  "private": true,
  "description": "An admin dashboard application demo built upon Ant Design and UmiJS",
  "dependencies": {
    "@ant-design/pro-layout": "^6.16.0",
    "@lingui/core": "^3.8.2",
    "@lingui/detect-locale": "^3.8.2",
    "@lingui/react": "^3.8.2",
    "ant-design-pro": "^2.1.1",
    "antd": "^4.15.0",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "classnames": "^2.2.6",
    "dva-model-extend": "^0.1.2",
    "echarts": "^4.2.0-rc.2",
    "echarts-for-react": "^2.0.15-beta.0",
    "echarts-gl": "^1.1.1",
    "echarts-liquidfill": "^2.0.2",
    "enquire-js": "^0.2.1",
    "json-format": "^1.0.1",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "react": "^17.0.2",
    "react-countup": "^4.0.0-alpha.6",
    "react-dom": "^17.0.2",
    "react-helmet": "^5.2.0",
    "react-perfect-scrollbar": "^1.4.2",
    "recharts": "^2.0.9",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@lingui/babel-preset-react": "^2.9.2",
    "@lingui/cli": "^3.8.2",
    "@lingui/loader": "^3.8.2",
    "@lingui/macro": "^3.8.2",
    "@umijs/preset-react": "^1.8.3",
    "@umijs/preset-ui": "^2.2.9",
    "babel-eslint": "^10.0.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-import": "^1.10.0",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^1.1.4",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^8.0.4",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.15.2",
    "stylelint": "^9.8.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "typescript": "^4.2.3",
    "umi": "^3.4.8"
  },
  "optionalDependencies": {
    "puppeteer": "^1.10.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --ext .js --fix",
      "npm run prettier",
      "git add"
    ],
    "**/*.less": [
      "stylelint --syntax less",
      "npm run prettier",
      "git add"
    ]
  },
  "lingui": {
    "locales": [
      "en",
      "zh"
    ],
    "sourceLocale": "en",
    "catalogs": [
      {
        "path": "src/locales/{locale}/messages",
        "include": [
          "src"
        ],
        "exclude": [
          "**/node_modules/**"
        ]
      }
    ],
    "orderBy": "origin",
    "runtimeConfigModule": [
      "@/i18n",
      "i18n"
    ]
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "lint:js": "eslint --ext .js src",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "start": "cross-env umi dev",
    "test": "cross-env BABELRC=none umi test",
    "prettier": "prettier --write 'src/**/*.(js|less)'",
    "precommit": "lint-staged",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "trans": "lingui extract --clean && node ./scripts/translate.js",
    "doc": "docsify serve docs"
  }
}
